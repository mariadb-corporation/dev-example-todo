    "version": "0.2.0",
    "configurations": [
		{
			"type": "java",
			"name": "Launch R2dbcApiApplication",
			"request": "launch",
			"mainClass": "com.mariadb.todo.R2dbcApiApplication",
			"projectName": "todo"
		},
		{
			"type": "java",
			"name": "Launch JdbcApiApplication",
			"request": "launch",
			"mainClass": "com.mariadb.todo.JdbcApiApplication",
			"projectName": "backend"
		},
		{
			"args": [
				"task"
			],
			"name": "Gulp task",
			"program": "${workspaceFolder}/node_modules/gulp/bin/gulp.js",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"type": "pwa-node"
		},
		{
			"name": "Python: Current File",
			"type": "python",
			"request": "launch",
			"program": "${file}",
			"console": "integratedTerminal"
		}
	]
}